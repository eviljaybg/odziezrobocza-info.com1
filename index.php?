    <script>
function changegrupa(numer,razem) {
	for (var i = 1; i <= razem ; i++) {
		element = document.getElementById('kategorie_'+i);
		element.style.display = 'none';
		element = document.getElementById('kategorie_wszystkie');
		element.style.display = 'none';
	}
	element = document.getElementById('kategorie_'+numer);
	element.style.display = 'block';
}

function ustawkat(numer) {
	element = document.getElementById('kategorie_wszystkie');
	element.style.display = 'none';
	element = document.getElementById('kategorie_'+numer);
	element.style.display = 'block';
}
</script>
