<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
 <title>Projob</title>
 <script language="javascript" type="text/javascript">
function resize(){
var w,h,sx,sy;
w = document.images[0].width ;
h = document.images[0].height;
sy = screen.availHeight;
sx = screen.availWidth;
w += 8;
h += 51;
if(w > sx || h>sy){
window.resizeTo(sx,sy);
window.moveTo(0,0);
}else{
sx = (sx - w) / 2;
sy = (sy - h) / 2;
window.resizeTo(w,h);
window.moveTo(sx,sy);
}
}
   var arrTemp=self.location.href.split("?");
   var picUrl = (arrTemp.length>0)?arrTemp[1]:"";
 </script>
</head>
<body onLoad="resize()" style="margin:0px;padding:0px;background-color:#FFFFFF;">
 <script language="javascript" type="text/javascript">
 document.write("<a href=\"javascript: window.close();\"> <img src=\"" + picUrl + "\" border=\"0\"><\/a>");
 document.close();
 </script>
</body>
</html>